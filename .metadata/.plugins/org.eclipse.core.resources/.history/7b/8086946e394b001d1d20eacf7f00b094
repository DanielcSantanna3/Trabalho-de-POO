package applicacao;

public class Mago{

	public double ataque; 
	public double defesa;
	public double pdv;

	public double atacar() {
		double atacar = this.ataque;
		return atacar;
	}

	public double defender() {
		double defender = this.defesa;
		return defender;
	}
	public double pdv() {
		double atacar = this.pdv;
		return atacar;
	}
}
